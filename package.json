{
  "name": "@engjts/auth",
  "version": "1.2.0",
  "description": "Janus Token System (JTS) - A two-component authentication architecture for secure, revocable, and confidential API authentication",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "jts": "./dist/cli/index.cjs"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    },
    "./cli": {
      "types": "./dist/cli/index.d.ts",
      "import": "./dist/cli/index.mjs",
      "require": "./dist/cli/index.cjs",
      "default": "./dist/cli/index.mjs"
    },
    "./adapter": {
      "types": "./dist/stores/adapter.d.ts",
      "import": "./dist/stores/adapter.mjs",
      "require": "./dist/stores/adapter.cjs",
      "default": "./dist/stores/adapter.mjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "rm -rf dist && rollup -c && chmod +x dist/cli/index.cjs dist/cli/index.mjs",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "example": "ts-node-dev --respawn --transpile-only examples/basic-server.ts",
    "prepublishOnly": "npm run build && npm run test"
  },
  "keywords": [
    "jts",
    "janus",
    "token",
    "authentication",
    "jwt",
    "jws",
    "jwe",
    "security",
    "api",
    "oauth",
    "session",
    "auth",
    "bearer-token",
    "stateless",
    "revocation",
    "typescript",
    "express",
    "middleware"
  ],
  "author": "JTS Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ukungzulfah/jts-core.git"
  },
  "homepage": "https://github.com/ukungzulfah/jts-core.git#readme",
  "bugs": {
    "url": "https://github.com/ukungzulfah/jts-core.git/issues"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^4.17.21",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "@vitest/coverage-v8": "^4.0.14",
    "rollup": "^4.53.3",
    "rollup-plugin-dts": "^6.3.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^12.1.0",
    "cookie-parser": "^1.4.7"
  },
  "peerDependencies": {
    "express": ">=4.0.0",
    "ioredis": ">=5.0.0",
    "pg": ">=8.0.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    },
    "ioredis": {
      "optional": true
    },
    "pg": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
